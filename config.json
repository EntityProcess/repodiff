{
  "tiktoken_model": "gpt-4o",
  "filters": [
    {
      "file_pattern": "*Test*.cs",
      "context_lines": 100
    },
    {
      "file_pattern": "*.cs",
      "context_lines": 500,
      "include_method_body": true,
      "include_signatures": false
    },
    {
      "file_pattern": "*.xml",
      "context_lines": 10
    },
    {
      "file_pattern": "*",
      "context_lines": 3
    }
  ]
}
